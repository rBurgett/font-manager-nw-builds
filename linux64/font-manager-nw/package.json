{
  "name": "font-manager-nw",
  "version": "0.2.2",
  "description": "Provides access to the system font catalog using nw.js",
  "main": "build/Release/fontmanager",
  "dependencies": {
    "nan": "~2.1.0"
  },
  "devDependencies": {
    "mocha": "*"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "cd test && nw .",
    "install": "nw-gyp --target=0.12.3 configure && nw-gyp build"
